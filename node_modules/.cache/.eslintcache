eact_js/@squashapps/training/gateware/buyers/src/components/AddBuyers/AddBuyers.js",["72","73"],"import React, { useContext, useState, useEffect } from \"react\";\nimport \"./AddBuyers.css\";\nimport { Context } from \"../../store/ContextProvider\";\nimport ErrorMsgDispaly from \"../commonComponents/ErrorMsgDispaly\";\n\nimport TextFieldComponent, {\n  HalfSizeField,\n  TextAreaField,\n} from \"../TextField/TextFieldComponent\";\nimport ImgInput from \"../TextField/ImgInput\";\nimport { Button } from \"primereact/button\";\nimport uuid from \"uuid/dist/v1\";\n\nfunction AddBuyers({ setShow }) {\n  const [imgUrl, setImgUrl] = useState(\"\");\n  const [formData, setFormData] = useState({\n    name: \"\",\n    contactPerson: \"\",\n    contactNumber: \"\",\n    address: \"\",\n    postal: \"\",\n    faxNo: \"\",\n    phone: \"\",\n    emailId: \"\",\n    website: \"\",\n  });\n  const [city, setCity] = useState(\"\");\n  const [states, setStates] = useState(\"\");\n  const [country, setCountry] = useState(\"\");\n  const { globalState, dispatch } = useContext(Context);\n  const [formValidity, setFormValidity] = useState(true);\n\n  const cities = [\n    { name: \"New York\", code: \"NY\" },\n    { name: \"Rome\", code: \"RM\" },\n    { name: \"London\", code: \"LDN\" },\n    { name: \"Istanbul\", code: \"IST\" },\n    { name: \"Paris\", code: \"PRS\" },\n  ];\n  const countries = [\n    { name: \"Australia\", code: \"AU\" },\n    { name: \"Brazil\", code: \"BR\" },\n    { name: \"China\", code: \"CN\" },\n    { name: \"Egypt\", code: \"EG\" },\n    { name: \"France\", code: \"FR\" },\n    { name: \"Germany\", code: \"DE\" },\n    { name: \"India\", code: \"IN\" },\n    { name: \"Japan\", code: \"JP\" },\n    { name: \"Spain\", code: \"ES\" },\n    { name: \"United States\", code: \"US\" },\n  ];\n  const stateList = [\n    { name: \"Maharastra\" },\n    { name: \"Karnataka\" },\n    { name: \"Andra\" },\n    { name: \"TamilNadu\" },\n    { name: \"Kerala\" },\n  ];\n\n  const handleFormData = (e) => {\n    const { name, value } = e.target;\n    setFormData((prevState) => ({\n      ...prevState,\n      [name]: value,\n    }));\n  };\n\n  const handleCityChange = (e) => {\n    setCity(e.value);\n  };\n  const handleCountryChange = (e) => {\n    setCountry(e.value);\n  };\n  const handleStatesChange = (e) => {\n    setStates(e.value);\n  };\n\n  /*  useEffect(() => {\n    validateForm();\n  }, [formData, country, city, states]); */\n\n  const validateForm = () => {\n    const {\n      name,\n      contactPerson,\n      contactNumber,\n      address,\n      postal,\n      faxNo,\n      phone,\n      emailId,\n      website,\n    } = formData;\n    if (\n      name.length &&\n      contactNumber.length &&\n      contactPerson.length &&\n      address.length &&\n      postal.length &&\n      faxNo.length &&\n      phone.length &&\n      emailId.length &&\n      website.length &&\n      imgUrl.length &&\n      country.length &&\n      states.length &&\n      city.length\n    ) {\n      setFormValidity(true);\n      return true;\n    } else {\n      setFormValidity(false);\n      return false;\n    }\n  };\n\n  const handleSubmit = (e) => {\n    if (validateForm()) {\n      let payload = {\n        ...formData,\n        logo: imgUrl,\n        ongoing: true,\n        totalOrers: `+${Math.round(Math.random() * 300)}k`,\n        id: uuid(),\n      };\n      dispatch({ type: \"ADD_BUYER\", payload });\n      setShow(false);\n    } else {\n      window.alert(\"please check the in\");\n    }\n  };\n\n  return (\n    <div className=\"p-fluid p-formgrid p-grid px-3 \">\n      {!formValidity && <ErrorMsgDispaly />}\n      <ImgInput imgUrl={imgUrl} setImgUrl={setImgUrl} />\n      <TextFieldComponent\n        label=\"Buisness Name\"\n        value={formData.name}\n        name=\"name\"\n        onChange={handleFormData}\n      />\n      <div className=\"grid px-2\">\n        <HalfSizeField\n          label=\"Contact Person\"\n          value={formData.contactPerson}\n          name=\"contactPerson\"\n          onChange={handleFormData}\n        />\n        <HalfSizeField\n          label=\"Contact No\"\n          value={formData.contactNumber}\n          onChange={handleFormData}\n          name=\"contactNumber\"\n        />\n      </div>\n      <div className=\"grid px-2\">\n        <TextAreaField\n          label=\"Address\"\n          value={formData.address}\n          name=\"address\"\n          onChange={handleFormData}\n        />\n      </div>\n      <div className=\"grid px-2\">\n        <HalfSizeField\n          label=\"Postal\"\n          name=\"postal\"\n          value={formData.postal}\n          onChange={handleFormData}\n        />\n        <HalfSizeField\n          label=\"Country\"\n          value={country}\n          options={countries}\n          name=\"country\"\n          type=\"dropDown\"\n          onChange={handleCountryChange}\n        />\n      </div>\n      <div className=\"grid px-2\">\n        <HalfSizeField\n          label=\"State\"\n          options={stateList}\n          name=\"state\"\n          type=\"dropDown\"\n          value={states}\n          onChange={handleStatesChange}\n        />\n        <HalfSizeField\n          label=\"City\"\n          name=\"city\"\n          value={city}\n          type=\"dropDown\"\n          options={cities}\n          onChange={handleCityChange}\n        />\n      </div>\n      <div className=\"grid px-2\">\n        <HalfSizeField\n          label=\"Fax No\"\n          value={formData.faxNo}\n          name=\"faxNo\"\n          onChange={handleFormData}\n        />\n        <HalfSizeField\n          label=\"Phone No\"\n          name=\"phone\"\n          value={formData.phone}\n          onChange={handleFormData}\n        />\n      </div>\n      <div className=\"grid px-2\">\n        <HalfSizeField\n          label=\"Email Id\"\n          name=\"emailId\"\n          value={formData.emailId}\n          onChange={handleFormData}\n        />\n        <HalfSizeField\n          name=\"website\"\n          label=\"Website\"\n          value={formData.website}\n          onChange={handleFormData}\n        />\n        <div className=\"flex justify-content-between w-full px-2\">\n          <div className=\"p-col-6 small__orangefont\">+ Add address</div>\n          <div className=\"p-col-6 mt-7\">\n            <Button label=\"cancel\" className=\"cancel__btn mr-3\" />\n            <Button\n              label=\"submit\"\n              className=\"submit__btn\"\n              /* disabled={!formValidity} */\n              onClick={handleSubmit}\n            />\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default AddBuyers;\n","/home/abiram/react_js/@squashapps/training/gateware/buyers/src/components/DataTableComponent/dataTableTemplates.js",[],"/home/abiram/react_js/@squashapps/training/gateware/buyers/src/constant/buyersData.js",[],"/home/abiram/react_js/@squashapps/training/gateware/buyers/src/components/TextField/TextFieldComponent.js",[],"/home/abiram/react_js/@squashapps/training/gateware/buyers/src/components/TextField/ImgInput.js",[],"/home/abiram/react_js/@squashapps/training/gateware/buyers/src/store/ContextProvider.js",[],"/home/abiram/react_js/@squashapps/training/gateware/buyers/src/store/reduer.js",[],"/home/abiram/react_js/@squashapps/training/gateware/buyers/src/components/commonComponents/ErrorMsgDispaly.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":6,"column":10,"nodeType":"80","messageId":"81","endLine":6,"endColumn":14},{"ruleId":"78","severity":1,"message":"82","line":15,"column":8,"nodeType":"80","messageId":"81","endLine":15,"endColumn":14},{"ruleId":"78","severity":1,"message":"83","line":1,"column":39,"nodeType":"80","messageId":"81","endLine":1,"endColumn":48},{"ruleId":"78","severity":1,"message":"84","line":30,"column":11,"nodeType":"80","messageId":"81","endLine":30,"end